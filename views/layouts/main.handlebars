<body>

  {{{ body }}}


  <!-- Log In Modal Ready for React-->
  <div class="modal fade" id="loginModalReact" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <div class="root">

          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- Log In Modal-->
  <div class="modal fade" id="loginModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title">Existing Users, Log In</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <form>
            <div class="form-group">
              <label for="exampleInputEmail1">Email address</label>
              <input type="text" class="form-control" id="loginEmail" aria-describedby="emailHelp" placeholder="Enter Email or Username">
            </div>
            <div class="form-group">
              <label for="exampleInputPassword1">Password</label>
              <input type="password" class="form-control" id="loginPass" placeholder="Password">
            </div>
            <button type="submit" class="btn btn-primary" id="logInStatic">Log In</button>
            <a style="float: right;" href="#signUpModal" data-toggle="modal" data-dismiss="modal"><small>Register</small></a>
          </form>
        </div>
      </div>
    </div>
  </div>

  <!-- Sign Up Modal-->
  <div class="modal fade" id="signUpModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title">New Users, Sign Up Below</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <form>
            <div class="form-row">
              <div class="form-group col-md-6">
                <label for="signUpUsername">Username</label>
                <input type="text" class="form-control" id="signUpUsername" placeholder="Username">
              </div>
              <div class="form-group col-md-6">
                <label for="signUpEmail">Email</label>
                <input type="text" class="form-control" id="signUpEmail" placeholder="Email">
              </div>
              <div class="form-group col-md-6">
                <label for="signUpPass">Password</label>
                <input type="password" class="form-control" id="signUpPass" placeholder="Password">
              </div>
            </div>
            <div class="form-group">
              <div class="form-check">
                <input class="form-check-input" type="checkbox" id="gridCheck">
                <label class="form-check-label" for="gridCheck">
                  <small id="privacyHelp">I agree to the cryptoShop <a data-toggle="modal" data-target="#privacyPolicy" href="#">Privacy Policy</a></small>
                </label>
              </div>
            </div>
            <button type="submit" id="registerStatic" class="btn btn-primary">Register</button>
            <a data-toggle="modal" href="#loginModal" data-dismiss="modal" style="float: right;"><small>Already Registered?</small></a>
          </form>
        </div>
      </div>
    </div>
  </div>

